<?php
echo 'Hello World';
$myDb = JFactory::getDbo();
$myQuery = $myDb->getQuery(true);
$myQuery
    ->select(array('gsl_country', 'gsl_country'))
    ->from('GSL_PRODUCT')
    ->where('gsl_bridge = ' . $myDb->quote('value'));
$myDb->setQuery($myQuery);
$rows = $myDb->loadObjectList();
print_r($rows);
echo 1;
?>